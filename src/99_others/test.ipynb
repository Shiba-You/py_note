   "source": [
    "A = [3, 1, 2, 4, 0]\n",
    "def bubble_sort(A):\n",
    "  for i in range(len(A)):\n",
    "    for j in range(len(A)-1, i, -1):\n",
    "      if A[j] <= A[j-1]:\n",
    "        A[j], A[j-1] = A[j-1], A[j]\n",
    "        print(A)\n",
    "  return A\n",
    "\n",
    "A = bubble_sort(A)\n",
    "print(A)\n",
